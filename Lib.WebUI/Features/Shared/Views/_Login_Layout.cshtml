<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Lib.WebUI</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/js/bundles/@WebpackChunkNamer.GetCssFile("test")" />
    <link rel="stylesheet" href="~/js/bundles/@WebpackChunkNamer.GetCssFile("login")" />
    <link rel="stylesheet" href="~/js/bundles/@WebpackChunkNamer.GetCssFile("home")" />
    <link rel="stylesheet" href="~/js/bundles/@WebpackChunkNamer.GetCssFile("core")" />
    @RenderSection("BundleStyles", required: false)
</head>
<body>
    <div class="lib-content-container">
        <main role="main" class="text-center">
            @RenderBody()
        </main>
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script charset="utf-8" type="text/javascript" src="~/js/bundles/@WebpackChunkNamer.GetJsFile("core")"></script>

    @RenderSection("Scripts", required: false)
    @RenderSection("BundleScripts", required: false)
</body>
</html>
